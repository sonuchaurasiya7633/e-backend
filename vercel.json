{
  "version": 2,
  "builds": [
    { "src": "api/server.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/order", "methods": ["POST"], "dest": "/api/server.js" },
    { "src": "/payment", "methods": ["GET"], "dest": "/api/server.js" },
    { "src": "/customers", "methods": ["GET"], "dest": "/api/server.js" }
  ]
}
